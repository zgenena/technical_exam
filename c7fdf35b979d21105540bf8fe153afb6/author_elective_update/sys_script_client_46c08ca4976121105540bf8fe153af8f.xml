        <applies_extended>false</applies_extended>
        <condition/>
        <description>Make caller read only until company is filled</description>
        <field>u_caller</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>!Caller read only until company filled</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }
    var dependentField = g_form.getControl('u_caller');
    var dependentFieldReadonly = true;
    if (g_form.getValue('u_company')) {
        dependentFieldReadonly = false;
    }
    if (dependentField) {
        dependentField.setReadOnly(dependentFieldReadonly);
    }
}
]]></script>
